# Firebase Configuration
# Copy this file to .env and fill in your actual Firebase project values
# NEVER commit .env with real values to version control

VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Error Reporting Configuration (Optional)
# For Sentry: Set VITE_ERROR_REPORTING_DSN to your Sentry DSN
# For custom endpoint: Set VITE_ERROR_REPORTING_ENDPOINT to your error logging API URL
# Leave empty to disable production error reporting (errors will still be logged to console)
VITE_ERROR_REPORTING_DSN=
VITE_ERROR_REPORTING_ENDPOINT=
